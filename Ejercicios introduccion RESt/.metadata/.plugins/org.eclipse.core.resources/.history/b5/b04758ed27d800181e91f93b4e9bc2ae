package com.lbenitez.rest;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

@Entity
public class Employee {

	@Id 
	@GeneratedValue
	private Long id;
	private String name;
	private String role;
	public Employee(Long id, String name, String role) {
		super();
		this.id = id;
		this.name = name;
		this.role = role;
	} 
	
	
	
}
